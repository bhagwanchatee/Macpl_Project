{
    "name": "customer-appvar-suppinv",
    "version": "0.1.0",
    "private": true,
    "description": "Create Supplier Invoice - Scan",
    "keywords": [
        "ui5",
        "sapui5",
        "adaptation-project"
    ],
    "devDependencies": {
        "@sap/ux-ui5-tooling": "^1.20.2",
        "@ui5/cli": "^4.0.33",
        "@ui5/task-adaptation": "^1.3.0",
        "rimraf": "^5.0.5"
    },
    "scripts": {
        "build": "ui5 build --clean-dest",
        "start": "fiori run --open /test/flp.html#app-preview",
        "start-editor": "fiori run --open /test/adaptation-editor.html",
        "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml",
        "undeploy": "fiori undeploy --config ui5-deploy.yaml",
        "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true"
    }
}
